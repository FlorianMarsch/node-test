
<div
	layout="row"
    ng-show="active == 'angebote'">
	Du hast noch ein Budget von {{squad.money| number: 0}} Copa Credits zur Verf√ºgung.
</div>
<div
	layout="row"
	ng-show="active == 'angebote'">
	<md-list flex>
        <md-subheader class="md-no-sticky">Du hast {{offers.length}} Angebote erhalten</md-subheader>
        <md-list-item class="md-3-line" ng-repeat="offer in offers" ng-click="null">
          <img ng-src="{{offer.player.imageUrl}}" class="md-avatar" alt="{{offer.player.name}}" />
          <div class="md-list-item-text" layout="column">
            <h3>{{ offer.player.name }}</h3> 
            <h4>{{ offer.player.position }}</h4>
            <p>{{ offer.player.points}} Punkte</p>
          </div>
         
		</md-list-item>
	</md-list>
</div>
<div
	layout="row"
	ng-show="active == 'angebote'">
	<md-list flex>
        <md-subheader class="md-no-sticky">Du hast {{demands.length}} Angebote unterbreitet</md-subheader>
        <md-list-item class="md-3-line" ng-repeat="demand in demands" ng-click="changeBid(demand,$event)">
          <img ng-src="{{demand.player.imageUrl}}" class="md-avatar" alt="{{demand.player.name}}" />
          <div class="md-list-item-text" layout="column">
            <h3>{{ demand.player.name }}</h3> 
            <h4>{{ demand.player.position }}</h4>
            <p>{{ demand.player.points}} Punkte</p>
          </div>
         
		</md-list-item>
	</md-list>
</div>
