<md-dialog id="bidDialog" >
  <md-dialog-content>
    <md-list flex>
      <md-list-item class="md-3-line" aria-label="{{bid.player.name}}">
        <img ng-src="{{bid.player.imageUrl}}" class="md-avatar" alt="{{bid.player.name}}" />
  </md-list-item>


  <md-list-item >
{{bid.player.name }}
</md-list-item>
<md-list-item >
{{bid.player.position }}
</md-list-item>
<md-list-item >
{{bid.player.points| number: 0}} Punkte
</md-list-item>
<md-list-item >
{{bid.player.owner.name}}
</md-list-item>
<md-list-item >
{{bid.player.price | number: 0}} Credits
</md-list-item>


</md-list>
  </md-dialog-content>
  <md-dialog-actions>
    <md-button ng-click="bid()" class="md-primary">Close Dialog</md-button>
  </md-dialog-actions>
</md-dialog>